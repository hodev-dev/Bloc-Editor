    "@types/react-router-dom": "^5.1.4",
    "@types/request": "^2.48.5",
    "@types/uuid": "^7.0.2"
  },
  "dependencies": {
    "@types/cheerio": "^0.22.11",
    "@types/draft-js": "^0.10.41",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/lodash": "^4.14.155",
    "@types/puppeteer": "^3.0.0",
    "@types/puppeteer-core": "^2.0.0",
    "@types/react": "^16.9.38",
    "@types/react-color": "^3.0.4",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@types/request": "^2.48.5",
    "@types/uuid": "^8.0.0",
    "JSONStream": "^1.3.5",
    "ace-builds": "^1.4.11",
    "axios": "^0.19.2",
    "cheerio": "^1.0.0-rc.3",
    "chokidar": "^3.4.0",
    "dotenv": "^8.2.0",
    "draft-js": "^0.11.5",
    "electron-devtools-installer": "^3.0.0",
    "electron-is-dev": "^1.2.0",
    "electron-log": "^4.1.1",
    "electron-reload": "^1.5.0",
    "event-stream": "^4.0.1",
    "fs": "0.0.1-security",
    "fuse.js": "^6.4.0",
    "react": "^16.13.1",
    "react-ace": "^9.1.1",
    "react-dom": "^16.13.1",
    "react-fuzzy": "^1.0.0",
    "react-player": "^2.5.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "react-spring": "^9.0.0-beta.34",
    "react-thunk": "^1.0.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "request": "^2.88.2",
    "stream-json": "^1.5.0",
    "typescript": "~3.7.2",
    "uuid": "^7.0.2"
  },
  "scripts": {
    "start": "yarn watch & react-scripts start",
    "build": "react-scripts build",
    "watch": "npm run development -- --watch",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "dev": "npm run development",
    "prod": "npm run production",
    "postinstall": "electron-builder install-app-deps",
    "r:dev": "BROWSER=none yarn start",
    "app:dev": "concurrently \"yarn dev\" \"yarn build\" \"BROWSER=none yarn start\" \"tsc -p electron -w\" \"wait-on http://localhost:3000 && tsc -p electron &&  electron .\"",
    "app:build": "yarn dev && yarn build && tsc -p electron && electron-builder  --linux --windows",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.7.5",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.2",
    "electron": "^18.3.7",
    "electron-builder": "^22.5.1",
    "electron-packager": "^15.0.0",
    "electron-reloader": "^1.0.1",
    "laravel-mix": "^5.0.4",
    "laravel-mix-purgecss": "^5.0.0",
    "postcss-cli": "^7.1.0",
    "sass": "^1.26.3",
    "tailwindcss": "^1.2.0",
    "ts-loader": "^7.0.0",
    "vue-template-compiler": "^2.6.11",
    "wait-on": "^4.0.2",
    "webpack-cli": "^3.3.11"
  },
  "build": {
    "asar": true,
    "extends": null,
    "asarUnpack": "node_modules/puppeteer/.local-chromium/**/*",
    "linux": {
      "icon": "../icons/icon.png",
      "executableName": "Bloc Editor",
      "target": [
        "deb",
        "snap",
        "AppImage",
        "rpm"
      ]
    },
    "win": {
      "icon": "build/icons/icon.png",
      "target": "nsis"
    },
    "files": [
      "build/**/*",
      "assets/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}