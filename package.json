  "scripts": {
    "start": "yarn watch:css & react-scripts start",
    "build": "react-scripts build",
    "postinstall": "electron-builder install-app-deps",
    "r:dev": "BROWSER=none yarn start",
    "e:dev": "concurrently \"BROWSER=none yarn start\" \"tsc -p electron -w\" \"wait-on http://localhost:3000 && tsc -p electron &&  electron .\"",
    "e:build": "yarn build && tsc -p electron && electron-builder --linux --windows",
    "build:css": "postcss src/assets/index.css -o src/assets/tailwind.css",
    "watch:css": "postcss src/assets/index.css -o src/assets/tailwind.css",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.7.5",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.2",
    "electron": "^8.2.3",
    "electron-builder": "^22.5.1",
    "electron-reloader": "^1.0.1",
    "postcss-cli": "^7.1.0",
    "sass": "^1.26.3",
    "tailwindcss": "^1.2.0",
    "ts-loader": "^7.0.0",
    "vue-template-compiler": "^2.6.11",
    "wait-on": "^4.0.2",
    "webpack-cli": "^3.3.11"
  },
  "build": {
    "extends": null,
    "linux": {
      "target": [
        "AppImage"
      ]
    },
    "win": {
      "target": "nsis"
    },
    "files": [
      "build/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
